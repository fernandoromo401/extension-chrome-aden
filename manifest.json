{
    "name": "ADENdev",
    "description": "Extensi√≥n para ver los ID's de un usuario en Acropolis | AcropolisLAB",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon-16.png",
            "32": "images/icon-16.png",
            "48": "images/icon-16.png",
            "128": "images/icon-16.png"
        }
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "permissions": [
        "nativeMessaging"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-16.png",
        "48": "images/icon-16.png",
        "128": "images/icon-16.png"
    }
}